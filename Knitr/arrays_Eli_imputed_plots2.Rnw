csv <- cl2


clust_plot <- function(csv){
  nrows = dim(csv)[1]
  df <- melt(csv)
  df["Strain"] <- c(rep("10E", nrows*3), rep("10G", nrows*3), rep("EK0", nrows*3))
  df["Time"] <- c(rep(1, nrows), rep(2, nrows), rep(3,nrows))
  
  b <- ggplot(df, aes(x = Time, y = value, group = Strain))
  b <- b + geom_smooth(aes(color = Strain, fill = Strain), level = 0.95)
  b <- b + geom_hline(yintercept = 0, linetype="dashed", size = 2)
  b <- b + scale_color_manual(labels = c("10E", "10G(Q3417X)", expression("10E_"*Delta*"ap2-hs")), 
                                          values = c("#619CFF", "#F8766D", "#00BA38")) 
  b <- b + scale_fill_manual(values = c("#619CFF", "#F8766D", "#00BA38")) 
  b <- b + guides(fill = F)
  b <- b + theme_classic() + theme(text = element_text(size=20, family = "arial"))
  ggsave(b, filename = paste0(figPath, deparse(substitute(csv)), "_smooth.png"), device = "png", dpi = "retina")
  b <- b + theme(text = element_text(family = "sans")) # Changing font again for PDF output.
  b <- b + scale_x_continuous(breaks = c(1,2,3), expand = c(0, 0), labels=c("1.5h", "3h", "+2h"))
  print(b)
}

clust_plot(cl1)
clust_plot(cl2)
clust_plot(cl3)
clust_plot(cl4)
clust_plot(cl5)
clust_plot(cl6)

