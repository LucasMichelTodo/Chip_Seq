#### Max. Fold-Change (any TP) ####
## >3 FC genes
top_10E_up <- imputed_10E_difs[imputed_10E_difs$Max_Dif > log2(4) & !is.na(imputed_10E_difs$Max_Dif),]
top_10G_up <- imputed_10G_difs[imputed_10G_difs$Max_Dif > log2(4) & !is.na(imputed_10G_difs$Max_Dif),]
top_EK0_up <- imputed_EK0_difs[imputed_EK0_difs$Max_Dif > log2(4) & !is.na(imputed_EK0_difs$Max_Dif),]


