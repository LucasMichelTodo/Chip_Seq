d <- draw.triple.venn(area1 = dim(top_10E_up)[1], 
                 area2 = dim(top_10G_up)[1], 
                 area3 = dim(top_EK0_up)[1], 
                 n12 = length(s10E_s10G), n23 = length(s10G_EK0), n13 = length(s10E_EK0),
                 n123 = length(s10E_s10G_EK0), category = c("10E", "10G(Q3417X)", expression("10E_"*Delta*"ap2-hs")), 
                 lty = "blank", fill = c("#619CFF", "#F8766D", "#00BA38"), alpha = 0.5,
                 fontfamily = "sans",
                 cat.fontfamily = "sans")

ggsave(d, filename = paste0(figPath, "venn_all_up.png"), device = "png", dpi = "retina")
@
<<venn_alltimes_3fc_up_euler, warning=FALSE, message=FALSE, out.width="1\\linewidth", out.height=".4\\textheight", echo = FALSE>>=
