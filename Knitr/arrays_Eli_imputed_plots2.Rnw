## Create universe; only genes with GO BP annotations
all_tbl <-  select(org.Pf.plasmo.db, keys = featureNames(xgene_impute), columns = c("GENENAME", "GO"), keytype = "SYMBOL")
ids = keys(org.Pf.plasmo.db, "GO")
df = select(GO.db, keys(GO.db), "ONTOLOGY")

bp_ids = ids[ids %in% df$GOID[df$ONTOLOGY == "BP"]]
universeBP <- unique(all_tbl[all_tbl$GO %in% bp_ids, "SYMBOL"])

cc_ids = ids[ids %in% df$GOID[df$ONTOLOGY == "CC"]]
universeCC <- unique(all_tbl[all_tbl$GO %in% cc_ids, "SYMBOL"])


mf_ids = ids[ids %in% df$GOID[df$ONTOLOGY == "MF"]]
universeMF <- unique(all_tbl[all_tbl$GO %in% mf_ids, "SYMBOL"])

# Remove genes that have go terms that do not appear in GO.db
noGO <- all_tbl[!all_tbl$GO %in% keys(GO.db),]$SYMBOL

universeBP <- universeBP[!universeBP %in% noGO]
universeCC <- universeCC[!universeCC %in% noGO]
universeMF <- universeMF[!universeMF %in% noGO]

## Run FE (hyperGTest) for each cluster
clust <- read.csv(file = "/media/lucas/Disc4T/Projects/Arrays_Eli/Motifs/FC4_all_6Clusters_Spearmann.csv", header = T, stringsAsFactors = F)
outDir <- "/media/lucas/Disc4T/Projects/Arrays_Eli/FE_GSEA/"
for (i in 1:6){
  cl <- clust[clust$Cluster == i,]$Gene
  cl_bp <- cl[cl %in% universeBP]
  cl_cc <- cl[cl %in% universeCC]
  cl_mf <- cl[cl %in% universeMF]
  
  hgCutoff <- 0.05
  paramsBP <- new("GOHyperGParams",
               geneIds=cl_bp,
               universeGeneIds=universeBP,
               annotation="org.Pf.plasmo.db",
                 ontology="BP",
               pvalueCutoff=hgCutoff,
               conditional=FALSE,
               testDirection="over")
  
  paramsCC <- new("GOHyperGParams",
               geneIds=cl_cc,
               universeGeneIds=universeCC,
               annotation="org.Pf.plasmo.db",
                 ontology="CC",
               pvalueCutoff=hgCutoff,
               conditional=FALSE,
               testDirection="over")
  
  paramsMF <- new("GOHyperGParams",
               geneIds=cl_mf,
               universeGeneIds=universeMF,
               annotation="org.Pf.plasmo.db",
                 ontology="MF",
               pvalueCutoff=hgCutoff,
               conditional=FALSE,
               testDirection="over")
  
  hgOverBP <- hyperGTest(paramsBP)
  write.table(summary(hgOverBP), file = paste0(outDir, "cluster_", i, "_BP.csv"), quote = F, sep = "\t", row.names = F)
  
  hgOverCC <- hyperGTest(paramsCC)
  write.table(summary(hgOverCC), file = paste0(outDir, "cluster_", i, "_CC.csv"), quote = F, sep = "\t", row.names = F)
  
  hgOverMF <- hyperGTest(paramsMF)
  write.table(summary(hgOverMF), file = paste0(outDir, "cluster_", i, "_MF.csv"), quote = F, sep = "\t", row.names = F)
  
}


